<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>homeServlet</servlet-name>
        <servlet-class>controller.homeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>productjobServlet</servlet-name>
        <servlet-class>controller.productjobServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filterCategoryController</servlet-name>
        <servlet-class>controller.filterCategoryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>controller.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>businessregistration</servlet-name>
        <servlet-class>businessregistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>businessregistrationServlet</servlet-name>
        <servlet-class>controller.businessregistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>companyinformationServlet</servlet-name>
        <servlet-class>controller.companyinformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>controller.search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>detailJob</servlet-name>
        <servlet-class>detailJob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>detailjob</servlet-name>
        <servlet-class>controller.detailjob</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homeServlet</servlet-name>
        <url-pattern>/homeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>productjobServlet</servlet-name>
        <url-pattern>/productjobServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filterCategoryController</servlet-name>
        <url-pattern>/filterCategoryController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>businessregistration</servlet-name>
        <url-pattern>/businessregistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>businessregistrationServlet</servlet-name>
        <url-pattern>/businessregistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>companyinformationServlet</servlet-name>
        <url-pattern>/companyinformationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>detailJob</servlet-name>
        <url-pattern>/detailJob</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>detailjob</servlet-name>
        <url-pattern>/detailjob</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>homeServlet</welcome-file>
    </welcome-file-list>
</web-app>
